	"admin_file": "admin.js",
	"logger": {
		"LEVEL": "info",
		"PATH": "/tmp/",
		"MAX_SIZE": 134217728
	},
	"default_connector": "rest+ws",
	"connector": {
		"rest": {
			"host": "localhost",
			"port": 80,
